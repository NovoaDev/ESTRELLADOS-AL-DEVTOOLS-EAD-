                "path": "./snippets/AL/WSAPI.json"
            },            {
                "language": "al",
                "path": "./snippets/AL/Translations.json"
            }
        ]
    },
    "author": {
        "name": "Antonio Novoa",
        "url": "https://www.tipsdevbusinesscentral.com",
        "email": "antonio.novoa@outlook.es"
    },
    "keywords": [
        "AL Language",
        "AL-DEVTOOLS",
        "AL Snippets",
        "ANJ"
    ],
    "dependencies": {
        "fs-extra": "^9.1.0",
        "xml2js": "^0.4.23"
    },
    "extensionPack": [
        "waldo.crs-al-language-extension",
        "rasmus.al-var-helper",
        "andrzejzwierzchowski.al-code-outline",
        "martonsagi.al-object-designer",
